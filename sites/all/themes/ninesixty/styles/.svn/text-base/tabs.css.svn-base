body{
	
	margin:0;
	padding:0;
}

#content{
	height:500px;
	width:100%;
	background-color:#fff;
	clear:left;
}

.top-tabs{
	border:none;
}

ul.tabs{
	width:100%;
	
	margin:0;
	list-style:none;
	float:left;
	border:none;
}

ul.tabs li{
	
	float:left;
}

ul.tabs li a {
	float:left;
	text-decoration: none;
	color: #ccc;
	padding: 12px 15px 0 0;
	margin-right: 4px;
	font: 900 12px "Arial", Helvetica, sans-serif;
	font-variant:small-caps;
	border:0px;
}

ul.primary{
	border-bottom: 0 none !important;
}

ul.primary li.active a{
	border:0px;
	
}
ul.primary li.active a{
	color: #0d5f83;
	background:url("../images/sliding.gif") no-repeat top right;
	border:0px;
}

ul.primary li.active a span{
	color: #0d5f83;
	background:url("../images/sliding.gif") no-repeat top left;
}

ul.tabs li a span {
	float:left;
	padding-right:15px;
	display:block;
	margin-top: -12px;
	height: 29px;
}

ul.tabs li a, ul.tabs li a.current {
	color: #fff;
	background:url("../images/sliding.gif") no-repeat bottom right;
}

ul.tabs li a span, ul.tabs li a.current span {
	color: #fff;
	background:url("../images/sliding.gif") no-repeat bottom left;
}


ul.tabs li a:hover, ul.tabs li a.current {
	color: #0d5f83;
	background:url("../images/sliding.gif") no-repeat top right;
	border:0px;
	
	
}

ul.tabs li a:hover span, ul.tabs li a.current span {
	color: #0d5f83;
	background:url("../images/sliding.gif") no-repeat top left;
	
	
}


